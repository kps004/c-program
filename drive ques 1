#include <stdio.h>

int main()
{
   int n,i,j,k,r,t;
   scanf("%d",&n);
   for(i=800;i<n;i=i+2)
   {    k=0;
       t=i;
       printf("%d\t",i);
       while(t)
       {
           r=t%10;
           t=t/10;
           k=k+r;
           
       }
       printf("%d\n",k);
       
   }
    return 0;
}
